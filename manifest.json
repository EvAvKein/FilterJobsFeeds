{
  "name": "Filter LinkedIn Job Search",
  "description": "An extension for filtering listings from LinkedIn's job-searching page",
  "version": "1.5",

  "permissions": ["storage"],
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": false
  },

  "content_scripts": [{
    "matches": ["https://www.linkedin.com/jobs/search/*", "https://angel.co/jobs"],
    "js": ["main.js"],
    "css": ["main.css"]
  }],
  
  "author": "Eve Aviv Keinan",

  "manifest_version": 3
}